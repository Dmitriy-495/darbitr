{
  "name": "OKX Futures",
  "wsUrl": "wss://ws.okx.com:8443/ws/v5/public",
  "enabled": true,
  "connection": {
    "pingIntervalMs": 25000,
    "pingFormat": {
      "request": "ping",
      "response": "pong"
    }
  },
  "subscribeMessages": [
    {
      "op": "subscribe",
      "args": [{ "channel": "books", "instId": "BTC-USDT" }]
    }
  ],
  "bidAskPaths": {
    "bid": "data.0.bids.0.0",
    "ask": "data.0.asks.0.0"
  }
}
